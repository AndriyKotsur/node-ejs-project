<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Catalogue item</title>
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
    <!-- @import header.html-->

    <div class="container">
        <div class="product-wrapper">
            <div class="product-column">
                <div class="slideshow-container" id="catalogue">
                    <div class="slider-title">Каталог наших товарів</div>
                    <div class="slideshow-items">
                        <div class="slides-item"><img src="../img/slides/rice.jpeg" alt="Slides photography"></div>
                        <div class="slides-item"><img src="../img/slides/grain.jpeg" alt="Slides photography"></div>
                        <div class="slides-item"><img src="../img/slides/grain2.jpeg" alt="Slides photography"></div>
                        <div class="slides-item"><img src="../img/slides/grain3.jpeg" alt="Slides photography"></div>
                        <a class="prev">&#10094;</a>
                        <a class="next">&#10095;</a>
                    </div>
                    <div class="slideshow-row">
                        <div class="slideshow-row-items"><img src="../img/slides/rice.jpeg" alt="Slides photography" id="1"></div>
                        <div class="slideshow-row-items"><img src="../img/slides/grain.jpeg" alt="Slides photography" id="2"></div>
                        <div class="slideshow-row-items"><img src="../img/slides/grain2.jpeg" alt="Slides photography" id="3"></div>
                        <div class="slideshow-row-items"><img src="../img/slides/grain3.jpeg" alt="Slides photography" id="4"></div>
                    </div>
                </div>
            </div>
            <div class="product-column"></div>
        </div>
    </div>
    <div class="modal-product">
        <span class="modal-close"></span>
        <img  class="modal-image" alt="Modal image">
    </div>
    <!-- @import footer.html-->
    <script src="../script/script.js"></script>
</body>
</html>